generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Entry {
  id          String   @id @default(cuid())
  date        String   // yyyy-mm-dd
  title       String
  consequence String
  url         String?
  createdAt   DateTime @default(now())
}

